<!doctype html>
<html>
<head>
  <title>开始支付</title>
  </head>
  <body>
   <script type="text/javascript">
  var isWeixin = function () { //判断是否是微信
        var ua = navigator.userAgent.toLowerCase();
            return ua.match(/MicroMessenger/i) == "micromessenger";
              };


  window.onload = function(){
        if(!isWeixin()){
                window.location.assign('<%=from_url%>');
                    }else{
                            var redirect_uri = encodeURI(window.location.href+"/back");
                                  window.location.assign('https://open.weixin.qq.com/connect/oauth2/authorize?appid=wx0564668ed5671740&redirect_uri='+redirect_uri+'&response_type=code&scope=snsapi_base&state=STATE#wechat_redirect');
                                      }
          }

  </script>




  </body>
  </html>

